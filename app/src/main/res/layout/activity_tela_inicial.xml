<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp"
    android:background="#EEE9E9"
    tools:context="ucdb.br.sweccamobile.TelaInicial">

    <TextView
        android:layout_width="wrap_content"
        android:textColor="#1E90FF"
        android:layout_height="wrap_content"
        android:text="Bem Vindo(a): "
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView6" />

    <TextView
        android:id="@+id/txtUser"
        android:textColor="@android:color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aguardando Nome"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView6"
        android:layout_toEndOf="@+id/textView6" />

    <TextView
        android:layout_width="wrap_content"
        android:textColor="#1E90FF"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/txtUser"
        android:text="ID: "
        android:id="@+id/textView5" />

    <TextView
        android:id="@+id/txtId"
        android:textColor="@android:color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aguardando ID"
        android:layout_below="@+id/textView6"
        android:layout_toRightOf="@+id/textView5"
        android:layout_toEndOf="@+id/textView5" />

    <ImageButton
        android:id="@+id/btn_ear_rot_ident"
        android:layout_width="140dp"
        android:layout_height="130dp"
        android:scaleType="fitXY"
        android:padding="0dp"
        android:background="@android:color/transparent"
        android:src="@drawable/teste"
        tools:ignore="ContentDescription"
        android:layout_marginBottom="19dp"
        android:layout_above="@+id/btn_storing"
        android:layout_toRightOf="@+id/textView5"
        android:layout_toEndOf="@+id/textView5"
        android:layout_centerVertical="true" />

    <ImageButton
        android:id="@+id/btn_mycotonxins"
        android:layout_width="140dp"
        android:layout_height="130dp"
        android:src="@drawable/teste"
        android:scaleType="fitXY"
        android:padding="0dp"
        android:background="@android:color/transparent"
        android:contentDescription=""
        tools:ignore="ContentDescription"
        android:layout_alignTop="@+id/btn_ear_rot_ident"
        android:layout_toRightOf="@+id/txtUser"
        android:layout_toEndOf="@+id/txtUser" />

    <ImageButton
        android:id="@+id/btn_storing"
        android:layout_width="140dp"
        android:layout_height="130dp"
        android:src="@drawable/teste"
        android:scaleType="fitXY"
        android:padding="0dp"
        android:background="@android:color/transparent"
        tools:ignore="ContentDescription"
        android:layout_alignTop="@+id/btn_ear_rot_man"
        android:layout_alignLeft="@+id/btn_ear_rot_ident"
        android:layout_alignStart="@+id/btn_ear_rot_ident"
        android:layout_centerVertical="true"/>

    <ImageButton
        android:id="@+id/btn_ear_rot_man"
        android:layout_width="140dp"
        android:padding="0dp"
        android:layout_height="130dp"
        android:src="@drawable/teste"
        android:scaleType="fitXY"
        android:background="@android:color/transparent"
        tools:ignore="ContentDescription"
        android:layout_marginBottom="151dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/btn_mycotonxins"
        android:layout_alignStart="@+id/btn_mycotonxins"
        android:layout_centerVertical="true"/>

    <ImageButton
        android:id="@+id/btn_contacts"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="19dp"
        android:src="@drawable/teste"
        android:scaleType="fitXY"
        android:background="@android:color/transparent"
        tools:ignore="ContentDescription"
        android:layout_below="@+id/btn_storing"
        android:layout_centerHorizontal="true" /></RelativeLayout>
